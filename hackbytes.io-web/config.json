{
  "port": 8080,
  "baseUrl": "/",
  "editor": "vim",
  "post_ext": "html",
  "comment_ext": "md",
  "comments_author": "anonymous",
  "filenameTemplate": "/:file/index.html",
  "locals": {
	"application": "nitelite.io",
	"url": "https://nitelite.io",
	"base_url": "nitelite.io",
	"title": "nitelite.io",
	"subtitle": "Wintersmith blog about software, productivity, and my tech projects.",
	"author": "Byron Sanchez",
	"description": "Wintersmith blog about software, productivity, and my tech projects.",
	"comments_enabled": true,
	"ratings_enabled": true,
	"skills_enabled": true,
	"keys_enabled": true,
	"related_entries_limit": 5,
	"date_format_post": "dddd, MMMM Do, YYYY",
	"date_format_comment": "MMMM Do, YYYY h:mm A",
	"email": "byron@hackbytes.io",
	"gravatar_url": "http://www.gravatar.com/avatar",
	"gravatar_size": 64,
	"gravatar_default": "identicon"
  },
  "plugins": [
	"./plugins/paginator.coffee",
	"./plugins/wintersmith-articles-helper.coffee",
	"./plugins/wintersmith-portfolio-helper.coffee",
	"./plugins/wintersmith-handleize-helper.coffee",
	"./plugins/wintersmith-titleize-helper.coffee",
	"./plugins/wintersmith-tag-pages.coffee",
	"./plugins/wintersmith-robotskirt.coffee",
	"./plugins/related_posts.coffee",
	"./plugins/wintersmith-webpack.coffee"
  ],
  "require": {
	"moment": "moment",
	"_": "underscore",
	"typogr": "typogr"
  },
  "jade": {
	"pretty": true
  },
  "most_popular_articles": {
	"limit": 5
  },
  "paginator": {
	"isEnabled": 1,
	"template": "layouts/index.jade",
	"perPage": 5
  },
  "articlesHelper": {
	"articles": "notebooks"
  },
  "portfolioHelper": {
	"portfolio": "portfolio"
  },
  "robotskirt": {
	"extensions": [
	  "ext_fenced_code",
	  "ext_no_intra_emphasis",
	  "ext_autolink",
	  "ext_strikethrough",
	  "ext_lax_spacing",
	  "ext_superscript",
	  "ext_tables"
	],
	"smart": true
  },
  "tagPages": {
	"perPage": 5,
	"template": "layouts/tags.jade",
	"filter": [
	  "5 stars",
	  "4 stars",
	  "3 stars",
	  "2 stars",
	  "1 star",
	  "art",
	  "edm",
	  "music",
	  "piano",
	  "singing"
	]
  },
  "webpack": {
	"output": "main.bundle.js"
  },
  "portfolio": {
	"tags": [
	  "angular",
	  "mobile",
	  "web"
	]
  },
  "pygments": true,
  "ignore": [
	"README.md",
	"README.org",
	"Gruntfile"
  ]
}

